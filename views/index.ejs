<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=0.8">
  <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  <title>CovidUpdateResults</title>
  <!-- css stylesheet -->
  <link href="styles.css" rel="stylesheet">

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://d3js.org/d3.v6.min.js"></script>
</head>

<body>

  <div class="site-wrapper">
    <div class="site-wrapper-inner">
      <div class="cover-container">
        <div class="inner cover">

          <h1 class="cover-heading"><%=location%></h1>

          <svg width="400px" height="400px"></svg>

          <h4 class="sub-titles blue">Confirmed:</h4>
          <p class="lead confirmed"><%=confirm%></p>

          <h4 class="sub-titles green">Recovered:</h4>
          <p class="lead recovered"><%=recover%></p>

          <h4 class="sub-titles red">Critical:</h4>
          <p class="lead critical"><%=crit%></p>

          <h4 class="sub-titles yellow">Fatalities:</h4>
          <p class="lead deaths"><%=fatal%></p>


          <input class="btn btn-default backButton" type="button" value="Back" onclick="window.history.back()" />


        </div>
      </div>
    </div>
  </div>

  <script src="diagram.js" type="text/javascript">

  </script>
</body>

</html>
